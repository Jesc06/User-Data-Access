@model StudentInformation


<form asp-controller="RegisterInfo" asp-action="create" method="post">
            <div class="form-group">
                <label>Name</label>
                <input asp-for="name" type="text" class="form-control"  placeholder="Name">
            </div>
                    <div class="form-group">
                        <label>Middlename</label>
                        <input asp-for="middlename" type="text" class="form-control"  placeholder="Middlename">
                    </div>
            <div class="form-group">
                <label>Lastname</label>
                <input asp-for="lastname" type="text" class="form-control"  placeholder="Lastname">
            </div>
        <button type="submit" class="btn btn-primary">Submit</button>
</form>



